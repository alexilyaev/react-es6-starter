{
  "name": "react-es6-starter",
  "version": "1.0.1",
  "description": "A boilerplate for React projects with Webpack and ES6",
  "scripts": {
    "start": "webpack-dev-server --hot --progress",
    "prebuild": "npm prune&npm install",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "lintjs": "eslint -c .eslintrc --ignore-path .gitignore --max-warnings 0 .",
    "lintcss": "stylelint --syntax scss 'app/**/*.scss'",
    "lint": "npm run lintjs && npm run lintcss",
    "eslint-find-unused-rules": "eslint-find-rules -u .eslintrc -n"
  },
  "license": "MIT",
  "dependencies": {
    "cssrecipes-defaults": "0.5.0",
    "normalize.css": "7.0.0",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-router-dom": "4.1.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.23.0",
    "clean-webpack-plugin": "0.1.16",
    "cross-env": "5.0.0",
    "css-loader": "0.28.2",
    "eslint": "3.19.0",
    "eslint-config-react": "1.1.7",
    "eslint-find-rules": "1.14.3",
    "eslint-loader": "1.7.1",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-compat": "1.0.2",
    "eslint-plugin-react": "7.0.1",
    "expose-loader": "0.7.3",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.5",
    "react-hot-loader": "3.0.0-beta.6",
    "sass-loader": "6.0.5",
    "style-loader": "0.18.1",
    "stylelint": "7.10.1",
    "stylelint-webpack-plugin": "0.7.0",
    "url-loader": "0.5.8",
    "webpack": "2.6.0",
    "webpack-dev-server": "2.4.5"
  }
}
