{
  "name": "react-es6-starter",
  "version": "0.0.0",
  "description": "A boilerplate for React projects with Webpack and ES6",
  "scripts": {
    "start": "webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack -p",
    "lintjs": "eslint -c .eslintrc --ignore-path .gitignore --max-warnings 0 .",
    "lintcss": "stylelint --syntax scss 'app/**/*.scss'",
    "lint": "npm run lintjs && npm run lintcss",
    "eslint-find-unused-rules": "eslint-find-rules -u .eslintrc -n",
    "stylelint-find-unused-rules": "stylelint-find-rules"
  },
  "license": "MIT",
  "dependencies": {
    "cssrecipes-defaults": "0.5.0",
    "normalize.css": "7.0.0",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "7.1.5",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "clean-webpack-plugin": "0.1.17",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "eslint": "4.8.0",
    "eslint-config-react": "1.1.7",
    "eslint-find-rules": "3.1.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-compat": "2.0.1",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "1.1.5",
    "friendly-errors-webpack-plugin": "1.6.1",
    "html-webpack-plugin": "2.30.1",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.7",
    "prettier": "1.7.4",
    "react-hot-loader": "3.0.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "stylelint": "8.2.0",
    "stylelint-find-rules": "1.0.3",
    "stylelint-webpack-plugin": "0.9.0",
    "url-loader": "0.6.2",
    "webpack": "3.6.0",
    "webpack-dev-server": "2.9.1"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "webpack",
    "boilerplate",
    "starter"
  ]
}
